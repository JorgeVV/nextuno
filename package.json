{
  "name": "nextuno",
  "version": "0.1.0",
  "description": "An opinionated boilerplate for Next.js",
  "main": "server/index.js",
  "repository": "https://github.com/JorgeVV/nextuno",
  "author": "Jorge Villalobos <jorge.villalobos2809@gmail.com> (https://jorgevv.github.io)",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "nodemon",
    "build": "next build && tsc --project tsconfig.server.json",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "lint:ts": "tslint --project tsconfig.json --fix",
    "lint:css": "stylelint \"{src,pages}/**/*.{css,scss}\" --fix",
    "analyze": "cross-env BUNDLE_ANALYZE=both npm run build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser npm run build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server npm run build"
  },
  "dependencies": {
    "cross-env": "^5.2.0",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "next": "^7.0.2",
    "next-seo": "^1.9.0",
    "nprogress": "^0.2.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.16.1",
    "@types/next": "^7.0.6",
    "@types/nprogress": "^0.0.29",
    "@types/react": "^16.7.20",
    "@types/react-dom": "^16.0.11",
    "@zeit/next-bundle-analyzer": "0.1.3-canary.1",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-sass": "^1.0.1",
    "@zeit/next-typescript": "^1.1.1",
    "copy-webpack-plugin": "^4.6.0",
    "ejs": "^2.6.1",
    "imagemin-gifsicle": "^6.0.1",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-optipng": "^6.0.0",
    "imagemin-svgo": "^7.0.0",
    "lqip-loader": "^2.2.0",
    "next-compose-plugins": "^2.1.1",
    "next-env": "^1.0.1",
    "next-offline": "^3.3.5",
    "next-optimized-images": "^2.3.3",
    "node-sass": "^4.11.0",
    "nodemon": "^1.18.9",
    "postcss-preset-env": "^6.5.0",
    "prettier": "^1.16.1",
    "responsive-loader": "^1.2.0",
    "sharp": "^0.21.3",
    "stylelint": "^9.10.1",
    "stylelint-a11y": "^1.1.10",
    "stylelint-config-recommended": "^2.1.0",
    "svg-sprite-loader": "^4.1.3",
    "ts-node": "^8.0.1",
    "tslint": "^5.12.1",
